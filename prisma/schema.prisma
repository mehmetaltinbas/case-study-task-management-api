generator client {
    provider = "prisma-client"
    output   = "../src/generated/prisma"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id           String  @id @default(uuid())
    userName     String  @unique
    passwordHash String
}

model Task {
    id String @id @default(uuid())
    
}